html
    head
        title login Page
        link(rel='stylesheet', href='/styles.css')
    body
        <div class="login">
        h1(class="log") Login Page
        br
        a(href='/signUp') signup for new account
        form(action='/homePage', method='POST')
            p Username:
                input#username(type="textbox",name="username")
            p Password:
                input#password(type="password", name="password")
            button#refresh(type="submit") submit

        </div>
