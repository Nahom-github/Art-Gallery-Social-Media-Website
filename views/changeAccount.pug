html
    head
        title Sign up
        link(rel='stylesheet', href='/styles.css')
    
    body
        h1 Please add an artwork to become an artist
        form(action='/change', method='POST')
            p title:
                input#artist(type="textbox",name="name")
            p Year:
                input#minattack(type="textbox",name="year")
            p Category:
                input#maxattack(type="textbox",name="category")
            p medium:
                input#minhealth(type="textbox",name="medium")
            p Description:
                input#minhealth(type="textbox",name="description")
            p image Link:
                input#minhealth(type="textbox",name="link")
            button#refresh(type="submit") Submit Artwork

        div#results